{
  "name": "@microdrop/state-saver",
  "version": "0.0.4",
  "description": "Save and restore the state of microdrop",
  "main": "src/state-saver.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "./node_modules/.bin/webpack",
    "build-dev": "npm run-script build-microdrop-async && npm run-script link-packages && webpack",
    "link-packages": "npm ln ../microdrop-async",
    "build-microdrop-async": "npm run-script --prefix ../microdrop-async build-dev",
    "update-deps": "ncu -u @microdrop/async @microdrop/device-controller @microdrop/ui-plugin && npm install",
    "git-add": "git add package-lock.json && git add -p . && git commit",
    "publish-pkg": "npm version patch && npm publish && git push origin master"
  },
  "keywords": [
    "Microdrop",
    "Save",
    "State",
    "Plugin"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "css-loader": "^0.28.7",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@microdrop/async": "^0.2.8",
    "@microdrop/ui-plugin": "^0.1.3",
    "jsoneditor": "^5.10.1",
    "lodash": "^4.17.4",
    "sillyname": "^0.1.0",
    "yo-yo": "^1.4.1"
  }
}
